	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="ObjectReader" class="tpsql.reader.DefaultObjectReader" >
	</bean>

	<bean id="ObjectWriter" class="tpsql.writer.DefaultObjectWriter" >
	</bean>
	
	<bean id="CollectionReader" class="tpsql.reader.DefaultCollectionReader" >
	</bean>
	
	<bean id="SpringContextAware" class="tpsql.spring.SpringContext" >
	</bean>

	<bean id="DataConvert" class="tpsql.convert.DataConvert" >
	</bean>
</beans>