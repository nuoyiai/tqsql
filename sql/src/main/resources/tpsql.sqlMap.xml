      <clause assert="!isNull({3})">{3}.</clause><![CDATA[{0} >= ]]>[{1}]
    </clause>
    <clause prepend="and" assert="isNotEmpty([{2}])" >
      <clause assert="!isNull({3})">{3}.</clause><![CDATA[{0} <= ]]>[{2}]
    </clause>
  </clause>
